BUTTON_ACCEPT=Aceptar
BUTTON_ACCEPT_INSTALL_TEXT=Aceptar e instalar
BUTTON_NEXT=Siguiente
BUTTON_LEAVE=Salir
BUTTON_FINISH=Finalizar
