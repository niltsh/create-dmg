[welcomePage]
WELCOME_TITLE=<p>Welcome to</p><br><span id="welcome_app_name">%APP_NAME%</span><br><p id="welcome_ver">%VERSION%</p>
WELCOME_TEXT_DESCRPITION=
PRODUCT_LINKS=<span id="tos_link_span"><a id="terms_of_use" target="_blank" onfocus="this.blur()" onclick="fileManager.openBrowser(this.href);" >Terms of Use</a></span><span id="privacy_link_span"><a id="privacy_policy" target="_blank" onfocus="this.blur()" onclick="fileManager.openBrowser(this.href);" >Privacy Policy</a></span><span id="eula_link_span"><a id="eulaLink" target="_blank" onfocus="this.blur()" onclick="fileManager.openBrowser(this.href);" >End User License Agreement</a></span><span id="sdk_privacy_link_span"><a id="sdk_privacy_policy" target="_blank" onfocus="this.blur()" onclick="fileManager.openBrowser(this.href);" >SDK - Privacy Policy</a></span>

[licensePage]
LICENSE_TITLE=License Agreement
LICENSE_FOOTER=If you accept the terms of the agreement, click "%BUTTON_ACCEPT%" to continue.<br />You must accept the Agreement to install the software.
PRIVACY_LINK=Privacy Policy
TOS_LINK=Terms of Use

[progressPage]
PROGRESS_TITLE=Downloading
PROGRESS_STATUS=%PROGRESS%%

[finishPage]
FINISH_TITLE=You've successfully installed
