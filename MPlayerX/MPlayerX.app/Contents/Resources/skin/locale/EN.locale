BUTTON_ACCEPT=Accept
BUTTON_OFFER_ACCEPT=Accept
BUTTON_ACCEPT_INSTALL_TEXT=Accept & Install
BUTTON_NEXT=Next
BUTTON_LEAVE=Leave
BUTTON_FINISH=Finish
BUTTON_SKIP=Skip

[Alerts]
